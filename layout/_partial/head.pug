link(rel="dns-prefetch" href="https://cdn.jsdelivr.net")
!=favicon_tag(config.favicon || theme.favicon || '/starry-favicon.ico')

if !theme.rss && config.feed && config.feed.path
  - theme.rss = config.feed.path
  != feed_tag(config.feed.path)

script#starry-config
  | var Starry = window.Starry || {};

if process.env.NODE_ENV !== 'dev' && theme.google_adsense && theme.google_adsense.enable
  include ../_third-party/google-adsense.pug

if process.env.NODE_ENV !== 'dev' && theme.google_analytics && theme.google_analytics.enable
  include ../_third-party/analytics/google-analytics.pug